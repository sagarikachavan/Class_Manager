<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"


    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FAD3E0"
    tools:context=".Assignments_More">

    <TextView
        android:id="@+id/submore"
        android:layout_width="184dp"
        android:layout_height="48dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="23dp"
        android:layout_marginLeft="23dp"
        android:layout_marginTop="80dp"
        android:fontFamily="@font/aref_ruqaa_bold"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/assignmore"
        android:layout_width="318dp"
        android:layout_height="128dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="48dp"
        android:layout_marginLeft="48dp"
        android:layout_marginTop="158dp"
        android:fontFamily="@font/aref_ruqaa" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="127dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="26dp"
        android:layout_marginLeft="26dp"
        android:layout_marginTop="349dp"
        android:fontFamily="@font/aref_ruqaa_bold"
        android:text="Submission Date"
        android:visibility="visible" />

    <TextView
        android:id="@+id/datemore"
        android:layout_width="165dp"
        android:layout_height="29dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="349dp"
        android:layout_marginEnd="29dp"
        android:layout_marginRight="29dp"
        android:fontFamily="@font/aref_ruqaa" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="43dp"
        android:layout_alignTop="@+id/textView16"
        android:layout_alignEnd="@+id/datemore"
        android:layout_alignRight="@+id/datemore"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="false"
        android:layout_alignParentRight="true"
        android:layout_marginStart="19dp"
        android:layout_marginLeft="19dp"
        android:layout_marginTop="68dp"
        android:layout_marginEnd="97dp"
        android:layout_marginRight="116dp"
        android:fontFamily="@font/aref_ruqaa_bold"
        android:text="Links to surf for help"
        android:visibility="visible" />

    <TextView
        android:id="@+id/linkmore"
        android:layout_width="284dp"
        android:layout_height="211dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginBottom="47dp"
        android:fontFamily="@font/aref_ruqaa" />

</RelativeLayout>